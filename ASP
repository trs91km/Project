Sub SubmitPhone
	if document.form1.SrPhone.value <> "" then
		document.form1.status.value = ""
		document.form1.submit
	else
		msgbox "Enter the Phone No. to Find"
	end if
END Sub

sub window_onload()
	radionchange
	Shipto_onClick
	Ship1_onClick
	Ship2_onClick
end sub


SUB radionchange
HideFrame(form1.all.DivCredit)
HideFrame(form1.all.DivBank)
	if form1.PayMode(0).checked = true then
		ShowFrame(form1.all.DivCredit)
	else
		ShowFrame(form1.all.DivBank)		
	end if	
end sub

sub ProductAmt_onkeypress()
	if not isnumeric(form1.ProductAmt.value & chr(window.event.keyCode)) then window.event.keyCode = 0
end sub

sub Shipping_onkeypress()
	if not isnumeric(form1.Shipping.value & chr(window.event.keyCode)) then window.event.keyCode = 0
end sub
sub Tax_onkeypress()
	if not isnumeric(form1.Tax.value & chr(window.event.keyCode)) then window.event.keyCode = 0
end sub


sub Shipto_onClick()
	if form1.shipto.checked = true then 
		ShowFrame(form1.all.DivShipto)
	else
		HideFrame(form1.all.DivShipto)
	end if
end sub

sub Ship1_onClick()
	if form1.ship1.checked = true then 
		ShowFrame(form1.all.DivShip1)
	else
		HideFrame(form1.all.DivShip1)
	end if
end sub

sub Ship2_onClick()
	if form1.ship2.checked = true then 
		ShowFrame(form1.all.DivShip2)
	else
		HideFrame(form1.all.DivShip2)
	end if
end sub

sub ShowFrame(FrameName)
	framename.style.visibility = "visible"
	framename.style.position = ""
end sub

sub HideFrame(FrameName)
	FrameName.style.visibility = "hidden"
	FrameName.style.position = "absolute"
end sub

//-->
</SCRIPT>

<SCRIPT LANGUAGE=javascript type="text/JavaScript">

function ddl()
	{
		var x = document.all("cavitywalls");
		var y = x.options[x.selectedIndex].text;
		if(y == "Yes")
		{
			document.form1.Ship1.disabled = true;
			alert("No more Cavity Questions");
		}
		if(y == "No")
		{
			document.form1.Ship1.disabled = false;
		}	
	}
	
	function ddr()
	{
		var x = document.all("Insutenyrs");
		var y = x.options[x.selectedIndex].text;
		if(y == "Yes")
		{
			document.form1.paymode[0].disabled = true;
			alert("No more Loft Questions");
		}
		if(y == "No")
		{
			document.form1.paymode[0].disabled = false;
		}
	}
	
	function dds()
	{
		var x = document.all("InsuJoist");
		var y = x.options[x.selectedIndex].text;
		if(y == "Above")
		{
			document.form1.paymode[0].disabled = true;
			document.form1.InsuLoftTen.disabled = true;
			document.form1.InsuJoist.disabled = true;
			document.form1.InsuLoftBoard.disabled = true;
			document.form1.InsuLoftEmp.disabled = true;
			document.form1.InsuWorkLoft.disabled = true;
			document.form1.InsuLoftConv.disabled = true;
			document.form1.paymode[1].checked = true;
			alert("No more Loft Questions");
		}
		else
		{
			document.form1.paymode[0].disabled = false;
		}
	}

	function ddsss()
	{
		var x = document.all("disposition");
		var y = x.options[x.selectedIndex].text;
		if(y == "DGI Insulation")
		{
			if((document.getElementById("ziparea").value == "B")||(document.getElementById("ziparea").value == "BA")||(document.getElementById("ziparea").value == "BS")||(document.getElementById("ziparea").value == "CF")||(document.getElementById("ziparea").value == "CH")||(document.getElementById("ziparea").value == "CV")||(document.getElementById("ziparea").value == "CW")||(document.getElementById("ziparea").value == "DE")||(document.getElementById("ziparea").value == "DY")||(document.getElementById("ziparea").value == "GL")||(document.getElementById("ziparea").value == "HR")||(document.getElementById("ziparea").value == "LE")||(document.getElementById("ziparea").value == "MK")||(document.getElementById("ziparea").value == "NG")||(document.getElementById("ziparea").value == "NN")||(document.getElementById("ziparea").value == "NP")||(document.getElementById("ziparea").value == "OX")||(document.getElementById("ziparea").value == "PE")||(document.getElementById("ziparea").value == "SA")||(document.getElementById("ziparea").value == "SN")||(document.getElementById("ziparea").value == "ST")||(document.getElementById("ziparea").value == "SY")||(document.getElementById("ziparea").value == "TA")||(document.getElementById("ziparea").value == "TF")||(document.getElementById("ziparea").value == "WA")||(document.getElementById("ziparea").value == "WR")||(document.getElementById("ziparea").value == "WS")||(document.getElementById("ziparea").value == "WV")||(document.getElementById("ziparea").value == "LD")||(document.getElementById("ziparea").value == "LN")||(document.getElementById("ziparea").value == "SK")||(document.getElementById("ziparea").value == "SP")||(document.getElementById("ziparea").value == "S"))
			{
			document.form1.Submitform.disabled = false;
			}
			else
			{
			document.form1.Submitform.disabled = true;
			alert("You Can't Take this Postcode for DGI Insulation");
			}

		}

		if(y == "WarmGen Insulation")
		{
			if((document.getElementById("ziparea").value == "L")||(document.getElementById("ziparea").value == "M")||(document.getElementById("ziparea").value == "WA")||(document.getElementById("ziparea").value == "CF")||(document.getElementById("ziparea").value == "CH")||(document.getElementById("ziparea").value == "WN"))
			{
			document.form1.Submitform.disabled = false;
			}
			else
			{
			document.form1.Submitform.disabled = true;
			alert("You Can't Take this Postcode for WarmGen Insulation");
			}

		}

		if(y == "IntellyGen")
		{
			if((document.getElementById("ziparea").value == "B")||(document.getElementById("ziparea").value == "WS")||(document.getElementById("ziparea").value == "WV")||(document.getElementById("ziparea").value == "CV")||(document.getElementById("ziparea").value == "DE")||(document.getElementById("ziparea").value == "LE")||(document.getElementById("ziparea").value == "LE"))
			{
			document.form1.Submitform.disabled = false;
			}
			else
			{
			document.form1.Submitform.disabled = true;
			alert("You Can't Take this Postcode for IntellyGen");
			}

		}
		if(y == "Crest Surveying")
		{
			if((document.getElementById("ziparea").value == "GU")||(document.getElementById("ziparea").value == "SO")
			{
			document.form1.Submitform.disabled = false;
			}
			else
			{
			document.form1.Submitform.disabled = true;
			alert("You Can't Take this Postcode for Crest Surveying");
			}

		}
		
	}
	
	function ddssw()
	{
		var x = document.all("disposition");
		var y = x.options[x.selectedIndex].text;

		var xsw = document.all("Location");
		var ysw = xsw.options[xsw.selectedIndex].text;

		if(y == "" || ysw == "")
		{
			document.form1.Submitform.disabled = false;
		}

		if(y == "DGI Insulation" && ysw == "Solid Walls")
		{
			if((document.getElementById("ziparea").value == "B")||(document.getElementById("ziparea").value == "BA")||(document.getElementById("ziparea").value == "BS")||(document.getElementById("ziparea").value == "CF")||(document.getElementById("ziparea").value == "CV")||(document.getElementById("ziparea").value == "DE")||(document.getElementById("ziparea").value == "DY")||(document.getElementById("ziparea").value == "GL")||(document.getElementById("ziparea").value == "HR")||(document.getElementById("ziparea").value == "LE")||(document.getElementById("ziparea").value == "NP")||(document.getElementById("ziparea").value == "SA")||(document.getElementById("ziparea").value == "SN")||(document.getElementById("ziparea").value == "SY")||(document.getElementById("ziparea").value == "TF")||(document.getElementById("ziparea").value == "WS")||(document.getElementById("ziparea").value == "WV"))
			{
			document.form1.Submitform.disabled = false;
			}
			else
			{
			document.form1.Submitform.disabled = true;
			alert("You Can't Take this Postcode for DGI Insulation Solid Walls");
			}

		}

		if(y == "DGI Insulation" && ysw == "Boiler")
		{
			if((document.getElementById("ziparea").value == "B")||(document.getElementById("ziparea").value == "BS")||(document.getElementById("ziparea").value == "CF")||(document.getElementById("ziparea").value == "CV")||(document.getElementById("ziparea").value == "DY")||(document.getElementById("ziparea").value == "GL")||(document.getElementById("ziparea").value == "HR")||(document.getElementById("ziparea").value == "NP")||(document.getElementById("ziparea").value == "OX")||(document.getElementById("ziparea").value == "SA")||(document.getElementById("ziparea").value == "S")||(document.getElementById("ziparea").value == "ST")||(document.getElementById("ziparea").value == "TF")||(document.getElementById("ziparea").value == "WR")||(document.getElementById("ziparea").value == "WS")||(document.getElementById("ziparea").value == "WV"))
			{
			document.form1.Submitform.disabled = false;
			}
			else
			{
			document.form1.Submitform.disabled = true;
			alert("You Can't Take this Postcode for DGI Insulation Boiler");
			}

		}

		if(y == "DGI Insulation" && ysw == "Lead Qualified")
		{
			if((document.getElementById("ziparea").value == "B")||(document.getElementById("ziparea").value == "BA")||(document.getElementById("ziparea").value == "BS")||(document.getElementById("ziparea").value == "CF")||(document.getElementById("ziparea").value == "CH")||(document.getElementById("ziparea").value == "CV")||(document.getElementById("ziparea").value == "DY")||(document.getElementById("ziparea").value == "GL")||(document.getElementById("ziparea").value == "HR")||(document.getElementById("ziparea").value == "LD")||(document.getElementById("ziparea").value == "MK")||(document.getElementById("ziparea").value == "NP")||(document.getElementById("ziparea").value == "OX")||(document.getElementById("ziparea").value == "SA")||(document.getElementById("ziparea").value == "SN")||(document.getElementById("ziparea").value == "SP")||(document.getElementById("ziparea").value == "ST")||(document.getElementById("ziparea").value == "SY")||(document.getElementById("ziparea").value == "TA")||(document.getElementById("ziparea").value == "TF")||(document.getElementById("ziparea").value == "WR")||(document.getElementById("ziparea").value == "WS")||(document.getElementById("ziparea").value == "WV"))
			{
			document.form1.Submitform.disabled = false;
			}
			else
			{
			document.form1.Submitform.disabled = true;
			alert("You Can't Take this Postcode for DGI Insulation Free Cavity Leads");
			}

		}

		if(y == "EuroEngeryServices Insulation" && ysw == "Boiler")
		{
			if((document.getElementById("boiler_Type").value == "Gas-Wall Mounted Boiler"))
			{
			document.form1.Submitform.disabled = false;
			}
			else
			{
			document.form1.Submitform.disabled = true;
			alert("You Can't Take this Type of Boiler for EuroEngeryServices");
			}

		}
		
	}

	function ddsub()
	{
		var x = document.all("OwnProper");
		var y = x.options[x.selectedIndex].text;
		if(y == "No")		{
			document.form1.Submitform.disabled = true;
			alert("This is Invalid Record");
		}
		if(y == "Yes") 
		{
			document.form1.Submitform.disabled = false;
			alert("This is Valid Lead Proceed");
		}
	}
	function ddQsix()
	{
		var x = document.all("InsuCustAllow");
		var y = x.options[x.selectedIndex].text;
		if(y == "No")
		{
			document.form1.InsuBenefits.disabled = true;
			document.form1.InsuBenefits2.disabled = true;
			document.form1.InsuBenefits3.disabled = true;
		}
		if(y == "Yes")
		{
			document.form1.InsuBenefits.disabled = false;
			document.form1.InsuBenefits2.disabled = false;
			document.form1.InsuBenefits3.disabled = false;
		}
	}

	function ddLeadtype()
	{
		var x = document.all("InsuInterest");
		var y = x.options[x.selectedIndex].text;
		if(y == "Solid Walls")
		{
			document.form1.SWalls.disabled = false;
			document.form1.SWalls1.disabled = false;
			document.form1.SWalls2.disabled = false;
		}
		else
		{
			document.form1.SWalls.disabled = true;
			document.form1.SWalls1.disabled = true;
			document.form1.SWalls2.disabled = true;
		}
	}
	
	function ddQsix1()
	{
		var x = document.all("InsuBenefits");
		var y = x.options[x.selectedIndex].text;
		if(y == "State Pension Credit")
		{
			document.form1.InsuBenefits2.disabled = true;
			document.form1.InsuBenefits3.disabled = true;
		}
		if(y == "Child Tax Credit - Income Less Than £ 15,860")
		{
			document.form1.InsuBenefits2.disabled = true;
			document.form1.InsuBenefits3.disabled = true;
		}
		
		if(y == "Income-based job seeker's allowance")
		{
			document.form1.InsuBenefits3.disabled = true;
			document.form1.InsuBenefits2.disabled = false;
		}
		
		if(y == "Income related employment and support allowance")
		{
			document.form1.InsuBenefits3.disabled = true;
			document.form1.InsuBenefits2.disabled = false;
		}
		
		if(y == "Income support")
		{
			document.form1.InsuBenefits3.disabled = true;
			document.form1.InsuBenefits2.disabled = false;
		}

		if(y == "Working tax credit with a relevant income below £15860")
		{
			document.form1.InsuBenefits2.disabled = true;
			document.form1.InsuBenefits3.disabled = false;
		}
	}
	function ddbuilt()
	{
		var x = document.all("InsuBuiltYear");
		var y = x.options[x.selectedIndex].text;
		if(y == "Yes")
		{
			
			document.form1.InsuBuiltBefo.disabled = true;
			document.form1.InsuHaveCavity.disabled = true;			
			document.form1.InsuTimFra.disabled = true;
			document.form1.InsuTileCla.disabled = true;
			document.form1.InsuDampWall.disabled = true;			
			alert("Dont ask cavity questions");
		}
	}
	
