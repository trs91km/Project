			rs("ship1") = 1
			rs("InsuBuiltYear") = Request.Form("InsuBuiltYear")
			rs("InsuBuiltBefo") = Request.Form("InsuBuiltBefo")
			rs("InsuHaveCavity") = Request.Form("InsuHaveCavity")
			rs("InsuTimFra") = Request.Form("InsuTimFra")
			rs("InsuTileCla") = Request.Form("InsuTileCla")
			rs("InsuDampWall") = Request.Form("InsuDampWall")
		else
			rs("ship1") = 0
			rs("InsuBuiltYear") = ""
			rs("InsuBuiltBefo") = ""
			rs("InsuHaveCavity") = ""
			rs("InsuTimFra") = ""
			rs("InsuTileCla") = ""
			rs("InsuDampWall") = ""
		end if
		
		if Request.Form("ship2") = "on" then
			rs("ship2") = 1
			''rs("CProducts1") = Request.Form("CProducts1")
			''rs("CProducts2") = Request.Form("CProducts2")
			rs("InsuBedRoom") = Request.Form("InsuBedRoom")
			''rs("InsuBedRoom1") = Request.Form("InsuBedRoom1")
			rs("InsuPrice") = Request.Form("InsuPrice")
			rs("InsuPInterest") = Request.Form("InsuPInterest")			
			'rs("InsuShower") = Request.Form("InsuShower")
			rs("InsuPwd") = Request.Form("InsuPwd")	
			rs("InsuBTCall") = Request.Form("InsuBTCall")
			rs("InsuBTCall1") = Request.Form("InsuBTCall1")
			if isdate(Request.Form("cmonth2")& "/" & Request.Form("cday2") & "/" & Request.Form("cYear2")) then
				rs("Demo2") = cdate(Request.Form("cmonth2")& "/" & Request.Form("cday2") & "/" & Request.Form("cYear2"))
			else
				rs("Demo2") = null
			end if		
			rs("CTime2") = Request.Form("CTime2")					
		else
			rs("ship2") = 0
			''rs("CProducts1") = ""
			''rs("CProducts2") = ""
			rs("InsuBedRoom") = ""
			''rs("InsuBedRoom1") = ""
			rs("InsuPrice") = ""
			rs("InsuPInterest") = ""
			'rs("InsuShower") = ""
			rs("InsuPwd") = ""
			rs("InsuBTCall") = ""
			rs("InsuBTCall1") =""			
			rs("Demo2") = null
			rs("CTime2") = null
		end if
		
		if Request.Form("Pending") = "on" then
			rs("Pending") = 1
		else
			rs("Pending") = 0
			rs("Check_by") = null
		end if
		
		if Request.Form("actype") = "QC" then 
			rs("Check_by") = session("agentid")
			mess = "The Customer <FONT size=4 color=Navy>" & rs("CustId")  & "</FONT> was Approved Successfully"
			RedString = "Insulation2013Qc.asp?mess= " & mess
		else
			RedString = "InsulationList.asp?mess= " & mess
		end if
			if isdate(Request.Form("smonth")& "/" & Request.Form("sday") & "/" & Request.Form("sYear")) then
			if isdate(Request.Form("smonth")& "/" & Request.Form("sday") & "/" & Request.Form("sYear") ) then
				rs("Made_at") = cdate(Request.Form("smonth")& "/" & Request.Form("sday") & "/" & Request.Form("sYear"))
			else
				rs("Made_at") = cdate(Request.Form("smonth")& "/" & Request.Form("sday") & "/" & Request.Form("sYear"))
			end if
			rs("EstTime") = Request.Form("EstTime") 
		end if
		rs("disposition") = Request.Form("disposition")	
		rs("ppiloan") = Request.Form("ppiloan")	
		rs("Location") = Request.Form("Location")	
		
		rs("Verified") = Request.Form("Verified")																
		rs("remarks") = trim(Request.Form("remarks"))
		'rs("SpeciTime") = trim(Request.Form("SpeciTime"))		
		rs("Qremarks") = trim(Request.Form("Qremarks"))
		rs("deleted") = 0
		rs.Update
		rs.Close
	end if
	Response.Redirect(RedString)
end if%>
